  "directions": [
    {
      "_id": "5a246993bb101c58d6968086",
      "step": "Preheat oven to 375F."
    },
    {
      "_id": "5a246b6f2c912bb7ffba8349",
      "step": "Heat the ghee and saut√© onions until translucent."
    },
    {
      "_id": "5a246b962c912bb7ffba834a",
      "step": "Add mushrooms and cook until moisture evaporates."
    },
    {
      "_id": "5a246bb02c912bb7ffba834b",
      "step": "Season with salt and pepper to taste and cool to room temperature."
    },
    {
      "_id": "5a246bcb2c912bb7ffba834c",
      "step": "Beat the eggs with the coconut milk, coconut flour and salt and pepper to taste until mixed."
    },
    {
      "_id": "5a246be82c912bb7ffba834d",
      "step": "Grease two muffin tins and line each cup with ham, taking care to cover the bottom and sides completely."
    },
    {
      "_id": "5a246c152c912bb7ffba834e",
      "step": "Fill each cup about 3/4 full and top each muffin with the halved cherry tomatoes."
    },
    {
      "_id": "5a246c322c912bb7ffba834f",
      "step": "Bake for about 20 minutes, flipping the tray after 10."
    },
    {
      "_id": "5a246c442c912bb7ffba8350",
      "step": "Cool the muffins in the pan for a few minutes before transferring to a wire rack."
    }
  ],

  "directions": [
    {
      "_id": "5a2470252c912bb7ffba837a",
      "step": "Heat the ghee and olive oil in a large saucepan over medium-high heat."
    },
    {
      "_id": "5a2470332c912bb7ffba837b",
      "step": "Add the onions, cabbage and carrots and 2 teaspoons salt."
    },
    {
      "_id": "5a2470502c912bb7ffba837c",
      "step": "Stir and cover the pot. Cook for 10 minutes."
    },
    {
      "_id": "5a24705e2c912bb7ffba837d",
      "step": "Add the potatoes, remaining salt, turmeric, cumin and pepper and stir well."
    },
    {
      "_id": "5a24707f2c912bb7ffba837e",
      "step": "Cover and reduce heat to medium low. Cook for 30 minutes or until all vegetables are tender."
    }
  ],
